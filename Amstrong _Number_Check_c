#include <stdio.h>
#include <math.h>

int main () {
    int n, rem, sum = 0, original

    printf("Enter a number: ");
    scanf("%d",&n);

    original = n;

    while(n !=0) {
        rem =n % 10;
        sum = sum + (rem*rem*rem);
    }
    if(sum==original)
       printf("Amstrong Number");
    else
       printf("Not Amstrong");

    return 0;
}
